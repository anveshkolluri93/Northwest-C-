                    var num = $('#num_courses').val();

                    var i = 0;
                    var html = '';
                    for (i = 1; i <= num; i++) {
                        //concatinate number of fields to a variable
                        html += 'Course ' + i + ': <input type="text" class="form-control" name="category-' + i + '"/><br/>';
                    }


                    $('#courseList').html(html);
                });
            });

